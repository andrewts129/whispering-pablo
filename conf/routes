
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET      /                                          controllers.Assets.at(path="/public", file="html/index.html")
GET      /api/get/numsubmissions                    controllers.api.ApiController.getNumberSubmissions
GET      /api/get/feed                              controllers.api.ApiController.getFeed
POST     /api/post/submit                           controllers.api.ApiController.postMessage(message: String)

      